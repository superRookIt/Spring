<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">


	<bean id="student" class="com.javalec.ex.Student">

		<constructor-arg value="1" />
		<constructor-arg value="김유신" />
		<constructor-arg value="100" />
		<constructor-arg value="90"></constructor-arg>
		<constructor-arg value="80"></constructor-arg>

	</bean>


	<bean id="student2" class="com.javalec.ex.Student">

		<constructor-arg value="2" />
		<constructor-arg value="홍길동" />
		<constructor-arg value="70" />
		<constructor-arg value="60"></constructor-arg>
		<constructor-arg value="50"></constructor-arg>

	</bean>


</beans>
